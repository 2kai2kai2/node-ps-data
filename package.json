{
  "name": "node-ps-data",
  "description": "Gets data about running processes by process id.",
  "author": {
    "name": "kaih2o",
    "url": "https://github.com/2kai2kai2"
  },
  "version": "1.0.6",
  "keywords": [
    "process",
    "processes",
    "ps",
    "pid",
    "lookup"
  ],
  "repository": {
    "url": "https://github.com/2kai2kai2/node-ps-data"
  },
  "dependencies": {
    "node-gyp": "^8.2.0",
    "node-gyp-build": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "node-addon-api": "^4.2.0",
    "typescript": "^4.4.3",
    "prebuildify": "^4.2.0"
  },
  "optionalDependencies": {
    "trim-newlines": "^4.0.2",
    "mem": "^9.0.1",
    "windows-build-tools": "^5.2.2"
  },
  "gypfile": true,
  "license": "GNU GPL",
  "main": "index.js",
  "module": "commonjs",
  "scripts": {
    "build-ts": "./node_modules/.bin/tsc",
    "build-linux": "./node_modules/.bin/prebuildify --napi --strip -m --platform linux",
    "build-win": "./node_modules/.bin/prebuildify --napi --strip -m --platform win32",
    "install": "./node_modules/.bin/node-gyp-build"
  },
  "type": "commonjs",
  "types": "index.d.ts"
}
